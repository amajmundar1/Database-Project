SELECT count(p.part_number) AS PartsCount
FROM part_nyc p
WHERE p.on_hand > 70;

SELECT (
	SELECT count(*) 
	FROM part_nyc p
	WHERE p.color = 0
	) + (
	SELECT count(*)
	FROM part_sfo p1
	WHERE p1.color = 0
	);

SELECT SUM(SumOnHand) 
FROM (
	SELECT (sum(p.on_hand)-sum(s.on_hand))+sum(s.on_hand) SumOnHand 
	FROM part_nyc p LEFT JOIN part_sfo s 
	WHERE p.part_number=s.part_number 
	GROUP BY p.part_number 
	HAVING p.color=0
     ) 
As Subtable;
